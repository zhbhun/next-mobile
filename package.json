{
  "name": "next-mobile",
  "version": "1.0.0",
  "description": "next config for mobile app",
  "homepage": "https://github.com/zhbhun/next-mobile",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhbhun/next-mobile"
  },
  "bugs": {
    "url": "https://github.com/zhbhun/next-mobile/issues"
  },
  "author": "zhbhun",
  "contributors": [
    {
      "name": "zhbhun",
      "url": "https://github.com/zhbhun"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && babel src --out-dir lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "next": "7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "eslint-config-ease-base": "^2.0.0",
    "lint-staged": "^7.3.0",
    "next": "7.0.2",
    "prettier-config-ease": "^2.0.0",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          },
          "spec": false,
          "loose": false,
          "modules": "commonjs",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "sourceType": "unambiguous"
  }
}
